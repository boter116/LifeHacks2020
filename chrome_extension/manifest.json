{
    "name": "Intentions",
    "version": "1.0",
    "description": "STREAM YUMMY",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "<all_urls>",
        "http://localhost:5000/"
      ],
    "background": {
        "scripts": ["selection.js"]
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }