{
    "name": "LifeHacks2020",
    "version": "1.0",
    "description": "STREAM YUMMY",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
      ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["selection.js"],
        "run_at": "document_start",
        "all_frames": true
    }],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }